

function appearance(weight:string,height:string,gender:string,address:string,no:number,state:string,country:string){
console.log("Below are details of the person");
console.log(weight);
console.log(height);
console.log(gender);
console.log(address);
console.log(no);
console.log(state);
console.log(country);
}
appearance("75 kg","5.7 ","Male","a colony ",7878788877,"Rajasthan","India")

