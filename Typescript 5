class appance{
Name:string;
Weight:string;

constructor(name:string,weight:string){
this.Name=name;
this.Weight=weight;
}

showinfo(){

console.log(this.Name);
console.log(this.Weight);

}

}
let det=new appance("aaa","75 kg");
det.showinfo();

