class Emi{
Type:string;
Rate:string;
constructor(type:string,rate:string){

this.Type=type;
this.Rate=rate;

}

display(){
    console.log(this.Type);
    console.log(this.Rate);
}


}

let how=new Emi("Personal","10 %")
how.display();


class onlineshopping
{
    username :string;
   password :string;
   emailid :string;
   mobilenumber :number;
   location :string;
   static loginname :string;
   static loginpwd :string;
   static productbrand :string;
   static  estimatedarrival :string;
   static productcolor :string;
   static returnpolicy :string;
   static fabric :string;
   static occassion :string
   static size :string;
   static gender :string;
  constructor(uname :string,pwd :string,mail :string,mobilenum :number,location :string)   //signup purpose
  {
    this.username = uname;
    this.password = pwd;
    this.emailid = mail;
    this.mobilenumber = mobilenum;
    this.location = location;
  }
  logincredentialscheck(logname :string,logpwd :string) :number
  {
    onlineshopping.loginname = logname;
    onlineshopping.loginpwd = logpwd;
    if( onlineshopping.loginname == this.username &&  onlineshopping.loginpwd ==  this.password)
    {
      return 1;
    }
    else
    {
      return 0;
    }
  }
 static productdetails(brand :string,color :string,material :string,occassion :string,size :string,gender :string)
  { onlineshopping.productbrand = brand;
    onlineshopping.productcolor = color;
    onlineshopping.fabric = material;
    onlineshopping.occassion = occassion;
    onlineshopping.size = size;
    onlineshopping.gender = gender;
    console.log("Product details saved successfully");   
  }
  static shippingdetails(arrival :string,repolicy :string)
  {
      onlineshopping.estimatedarrival = arrival;
      onlineshopping.returnpolicy = repolicy;
     console.log("Shipping details saved successfully");
  }
}
let akshay = new onlineshopping("Akk","saa@123","sya@gmail.com",9824367268,"Bangalore");
onlineshopping.shippingdetails("Monday","Not retyrnable");
onlineshopping.productdetails("Puma","Black","fabric","Party","S","Male");
akshay.logincredentialscheck("asas","asasad");

