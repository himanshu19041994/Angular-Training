//Typescript3


    class studentDetails{
        names:string;
        fathernames:string;
        mothernames:string;
        dobs:string;
        addresss:string;
        ages:number;
        sections:string;
        
    constructor(name:string,fathername:string,mothername:string,dob:string,address:string,age:number,section:string){
    this.names=name;
    this.fathernames=fathername;
    this.mothernames=mothername;
    this.dobs=dob;
    this.addresss=address;
    this.ages=age;
    this.sections=section;
    }
    basicinfo()
    {
   console.log(this.names," ",this.ages," ",this.sections);
    }
    static marks()
    {
    var sciencemarks:number=9;
    console.log(sciencemarks);
    }
    }
    let detail=new studentDetails("aks","RKS","saka","9-01-1999","ad colony",20,"A")
    detail.basicinfo()
    studentDetails.marks()
    
