class corporateInfo{
constructor(companyName,designation,salary,location,name,qualification,dob,address,residence,mobile,email,totalExp,skills,reportingManager,pancard){
this.companyName=companyName
this.designation=designation
this.salary=salary 
this.location=location
this.name=name
this.qualification=qualification
this.dob=dob
this.address=address
this.residence=residence
this.mobile=mobile
this.email=email
this.totalExp=totalExp
this.skills=skills
this.reportingManager=reportingManager
this.pancard=pancard
}
display(){
document.write(this.pancard)
}
}

