function employee(salary:number){
    
    return salary;
    }
    var ret:number=employee(5000);
    console.log(ret);
    
    function employeeName( name:string,city:string ){
        console.log(name," ",city)
    }
    employeeName("akshay","bengaluru")
    
    class employees{
        address:string;
        constructor(getaddress:string){
    this.address=getaddress;
    }
    displayaddress(){
        console.log(this.address);
    }
    }
    let pass=new employees("xy colony");
    pass.displayaddress();
    
    function studentMarks()
    {
    var maths:number=7
    var science:number=8
    var hindi:number=7
    var sanskrit:number=9
    var computer:number=7
    var social:number=9
    var sum:number=maths+hindi+science+sanskrit+computer+social
    return sum;
    }
    let total:number=studentMarks();
    console.log(total);
    
