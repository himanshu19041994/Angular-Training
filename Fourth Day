class internetservice{
constructor(invoice,Period,ServiceType,Description,Package,Rate,Unit,Quantity,Amount,Tax,Total){
    this.invoice= invoice 
    this.Period=Period
    this.ServiceType=ServiceType
    this.Description=Description
    this.Package = Package
    this.Rate = Rate
    this.Unit = Unit
    this.Quantity = Quantity
    this.Amount = Amount
    this.Tax = Tax
    this.Total = Total
}
}
var object=new internetservice(25689," 01/11/2015-30/11/2015 "," Internet Access ","Monthly Subscription","ACT Blaze","1,049.00 "," Per Month  ","30 Days ",1049,"140.71 ","1,189.71 ")

let  internetserviceprovider={
