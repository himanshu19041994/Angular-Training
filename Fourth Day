class jobInfo{
static currentcmpny="X technologies"
constructor(name,mobno,email){
this.Name=name
this.Mobno=mobno
this.Email=email
}
static showcurrentcompany(){
document.write(jobInfo.currentcmpny)
}

}
var show=new jobInfo("venka","7988888777","vd@gmail.com")
jobInfo.showcurrentcompany
