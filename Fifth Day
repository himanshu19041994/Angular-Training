class cardetails{
    static type="SUV"
    constructor(city,fueltype,kms,colour,owners,powersteering,insurance,manufacturingyear,dateofreg){
        this.City=city
        this.Fueltype=fueltype
        this.Kms=kms
        this.Colour=colour
        this.Owners=owners
        this.Powersteering=powersteering
        this.Insurance=insurance
        this.Manufacturingyear=manufacturingyear
        this.Dateofreg=dateofreg

}
static details(){
document.write(cardetails.type)
}

set display(colour){
this.cOlour=colour
}
get colo(){
return  this.colour

}

}
