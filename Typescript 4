class electricbills{
Uname:string;
Cname:string;
Address:string;
Pin:number;

constructor(uname:string,cname:string,address:string,pin:number){
this.Uname=uname;
this.Cname=cname;
this.Address=address;
this.Pin=pin;
}

disp(){
console.log(this.Uname);
console.log(this.Cname);
console.log(this.Address);
console.log(this.Pin);


}


}
let bill=new electricbills("1212!!","akash","v colony",323232);
bill.disp();

function mail(sender:string,sendermail:string,reciever:string,recievermail:string,day:string,date:string){
console.log("Below are the details of mail");

console.log("The senders name is "+sender);
console.log("senders id is "+sendermail);
console.log(reciever);
console.log(recievermail);
console.log(day);
console.log(date);

}
mail("Ram","ram@gmail","Shyam","Shyam@gmail","Tueday","12 aug")

function mailcheck(smail:string,rmail:string)
{

if (smail==undefined)
{
    return "Senders mail not valid"
}
else
{
    return "senders mail is valid"
}


}
let check:string=mailcheck("A @gmail","B@ gmail");
console.log(check);


