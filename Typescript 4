class mailinfo{
Sname:string;
Rname:string;

constructor(sndrname:string,rcvrname:string){
this.Sname=sndrname;
this.Rname=rcvrname;
}

shownames(){
console.log(this.Sname);
console.log(this.Rname);

}



}
let what=new mailinfo("ram","Shyam")
what.shownames();

function theatre(name:string,screen:number,movname:string,clas:string,ticket:number,seat:string,amount:number){

console.log("Details of tickets in PVR")
console.log(name)
console.log(screen)
console.log(movname)
console.log(clas)
console.log(ticket)
console.log(seat)
console.log(amount)
}
theatre("PVR",1,"sholay","Premium",12,"1 to 12", 250  )

function theatreamt(totalamount:number){

return totalamount
}

let am=theatreamt(2000);
console.log("The total amount of tickets is "+am);

let theatrein:any={
    TheatreName:"PVR",
    Screen:2,
    MovieName :"sholay",
    clas :"prem",
    PricePerTicket:250,
    NumberOfTkt :10,
    SeatNumbers :"25 to 35",
    
    totalamt:function(){
        console.log("The total amount is "+this.PricePerTicket*this.NumberOfTkt);

    }

  };

theatrein.methods=function():any{

console.log(this.TheatreName);
console.log(this.Screen);
console.log(this.MovieName);
console.log(this.clas);
console.log(this.SeatNumbers);

}
theatrein.methods();
theatrein.totalamt();



class theatreinfo {
   Tname:string;
   Mname:string;
    constructor(tname:string,movname:string) {
    this.Tname=tname;
    this.Mname=movname;
    }

    displayin(){
        console.log(this.Tname);
        console.log(this.Mname);
    }


}

let showme=new theatreinfo("Plaza","Sholay")
showme.displayin();
